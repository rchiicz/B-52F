<!-- Flaps -->
    <flap>
      <name>flaps</name>
      <mode>in-transit</mode>
      <path>Sounds/flaps.wav</path>
      <property>/surface-positions/left-flap-pos-norm</property>
      <volume>
        <factor>0.15</factor>
      </volume>
    </flap>

    <flap>
      <name>flaps</name>
      <mode>in-transit</mode>
      <path>Sounds/flaps.wav</path>
      <property>/surface-positions/right-flap-pos-norm</property>
      <volume>
        <factor>0.25</factor>
      </volume>
    </flap>

<!-- Spoilers -->
    <spoiler>
      <name>spoilers</name>
      <mode>in-transit</mode>
      <path>Sounds/flaps.wav</path>
      <property>/surface-positions/left-spoiler-pos-norm</property>
      <volume>
        <factor>0.15</factor>
      </volume>
    </spoiler>

    <spoiler>
      <name>spoilers</name>
      <mode>in-transit</mode>
      <path>Sounds/flaps.wav</path>
      <property>/surface-positions/right-spoiler-pos-norm</property>
      <volume>
        <factor>0.25</factor>
      </volume>
    </spoiler>

    <overspeed>
      <name>overspeed</name>
      <path>Sounds/overspeed2.wav</path>
      <mode>looped</mode>
      <condition>
        <or>
          <greater-than>
            <property n="0">/velocities/airspeed-kt</property>
            <property n="1">/limits/vmo</property>
          </greater-than>
          <greater-than>
            <property n="0">/velocities/mach</property>
            <property n="1">/limits/mmo</property>
          </greater-than>
        </or>
      </condition>
      <volume>
        <property>/velocities/airspeed-kt</property>
        <max>0.4</max>
      </volume>
    </overspeed>

    <gear-overspeed>
      <name>gear-overspeed</name>
      <path>Sounds/overspeed.wav</path>
      <mode>looped</mode>
      <condition>
        <property>/gear/gear[0]/position-norm</property>
        <greater-than>
          <property n="0">/velocities/airspeed-kt</property>
          <property n="1">/limits/max-gear-extension-speed</property>
        </greater-than>
      </condition>
      <volume>
        <property>/velocities/airspeed-kt</property>
        <max>0.4</max>
      </volume>
    </gear-overspeed>

    <flaps-overspeed>
      <name>flaps-overspeed</name>
      <path>Sounds/overspeed.wav</path>
      <mode>looped</mode>
      <condition>
        <property>/surface-positions/flap-pos-norm</property>
        <greater-than>
          <property n="0">/velocities/airspeed-kt</property>
          <property n="1">/limits/max-flap-extension-speed[0]/speed</property>
        </greater-than>
      </condition>
      <volume>
        <property>/velocities/airspeed-kt</property>
        <max>0.4</max>
      </volume>
    </flaps-overspeed>
    
    <stall>
      <name>stall</name>
      <path>Sounds/stall.wav</path>
      <mode>looped</mode>
      <condition>
        <greater-than>
          <property>/velocities/airspeed-kt</property>
          <value>40</value>
        </greater-than>
        <greater-than>
          <property>/orientation/alpha-deg</property>
          <value>10.0</value>
        </greater-than>
      </condition>
      <volume>
        <property>/velocities/airspeed-kt</property>
        <max>0.10</max>
      </volume>
    </stall>

  </fx>

</PropertyList>
