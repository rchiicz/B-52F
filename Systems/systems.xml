<?xml version="1.0"?>

<PropertyList>

  <electrical>
    <name>electrical</name>
    <number>0</number>
    <path>Aircraft/Generic/generic-electrical.xml</path>
  </electrical>

  <pitot>
    <name>pitot</name>
    <number>0</number>
  </pitot>

  <static>
    <name>static</name>
    <number>0</number>
    <tau>1</tau>
  </static>

  <!-- B-52F does not have engine/rpm property, so drive the vacuum off the electrical system
       so that generic instruments that need vacuum have it (e.g. the DG and HSI).
       (See: https://wiki.flightgear.org/Aircraft_systems#Vacuum)
  -->
  <vacuum>
    <name>vacuum</name>
    <number>0</number>
    <rpm>/systems/electrical/outputs/hsi</rpm>
    <scale>50.0</scale>
  </vacuum>

  <vacuum>
    <name>vacuum</name>
    <number>1</number>
    <rpm>/systems/electrical/outputs/hsi</rpm>
    <scale>50.0</scale>
  </vacuum>

</PropertyList>
